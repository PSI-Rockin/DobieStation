<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- configurations -->
  <ItemGroup Label="ProjectConfigurations">
    <ProjectConfiguration Include="Release Optimized|x64">
      <Configuration>Release Optimized</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|x64">
      <Configuration>Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Devel|x64">
      <Configuration>Devel</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Debug|x64">
      <Configuration>Debug</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <!-- globals -->
  <PropertyGroup Label="Globals">
    <ProjectGuid>
      {F61CEB41-9B79-417D-9908-0D91A97A18F8}</ProjectGuid>
    <WindowsTargetPlatformVersion>10.0.18362.0</WindowsTargetPlatformVersion>
    <ProjectName>InputCommon</ProjectName>
  </PropertyGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.Default.props" />
  <!-- configuration defaults -->
  <PropertyGroup Label="Configuration">
    <ConfigurationType>StaticLibrary</ConfigurationType>
    <CharacterSet>Unicode</CharacterSet>
    <PlatformToolset>v141</PlatformToolset>
  </PropertyGroup>
  <!-- release -->
  <PropertyGroup Condition="'$(Configuration)'=='Release'" Label="Configuration">
    <UseDebugLibraries>false</UseDebugLibraries>
  </PropertyGroup>
  <!-- optimized release -->
  <PropertyGroup Condition="'$(Configuration)'=='Release Optimized'" Label="Configuration">
    <UseDebugLibraries>false</UseDebugLibraries>
  </PropertyGroup>
  <!-- devel -->
  <PropertyGroup Condition="'$(Configuration)'=='Devel'" Label="Configuration">
    <UseDebugLibraries>false</UseDebugLibraries>
  </PropertyGroup>
  <!-- debug -->
  <PropertyGroup Condition="'$(Configuration)'=='Debug'" Label="Configuration">
    <UseDebugLibraries>true</UseDebugLibraries>
  </PropertyGroup>
  <!-- props -->
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.props" />
  <ImportGroup Label="ExtensionSettings">
    <Import Project="$(VCTargetsPath)\BuildCustomizations\masm.props" />
  </ImportGroup>
  <!-- prop includes -->
  <ImportGroup Label="PropertySheets">
    <Import Project="$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props" Condition="exists('$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props')" />
    <Import Project="common.props" />
  </ImportGroup>
  <PropertyGroup>
    <TargetName>$(ProjectName)$(Postfix)</TargetName>
  </PropertyGroup>
  <!-- default link -->
  <ItemDefinitionGroup>
    <Link>
      <OutputFile>$(BinDir)\$(ProjectName)$(Postfix).lib</OutputFile>
    </Link>
  </ItemDefinitionGroup>
  <!-- cpp files -->
  <ItemGroup>
    <ClCompile Include="$(InputDir)win_input.cpp" />
    <ClCompile Include="$(InputDir)common_input.cpp" />
  </ItemGroup>
  <!-- headers -->
  <ItemGroup>
    <ClInclude Include="$(InputDir)common_input.hpp" />
    <ClInclude Include="$(InputDir)win_input.hpp" />
  </ItemGroup>
  <!-- misc -->
  <ItemGroup>
    <None Include="$(RootDir)README.md" />
    <None Include="$(CoreDir)jitcommon\ir_instrlist.inc" />
    <None Include="$(RootDir)appveyor.yml" />
    <None Include="$(RootDir)CMakeLists.txt" />
    <None Include="$(RootDir)LICENSE" />
    <None Include="$(RootDir).gitignore" />
    <None Include="$(RootDir)CONTRIBUTING.md" />
    <None Include="application\application.pro" />
  </ItemGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
  <ImportGroup Label="ExtensionTargets">
    <Import Project="$(VCTargetsPath)\BuildCustomizations\masm.targets" />
  </ImportGroup>
</Project>