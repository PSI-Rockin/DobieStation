    </ClCompile>
    <ClCompile Include="$(ExtDir)\libdeflate\lib\deflate_decompress.c">
      <DisableSpecificWarnings>4127;4245;4100;4018;%(DisableSpecificWarnings)</DisableSpecificWarnings>
